<?xml version="1.0"?>
<odoo>
        <data>
                 <!-- Menu  dengan action -->
                <record id="action_type_kendaraan" model="ir.actions.act_window">
                        <field name="name">type kendaraan</field>
                        <field name="res_model">sysrental.type_kendaraan</field>
                        <field name="view_mode">list,form</field>
                        <field name="help" type="html">
                                <p class="o_view_nocontent_smiling_face">Create a new Data List</p>
                        </field>
                </record>
                <!-- views -->
                <record id="type_kendaraan_tree_view" model="ir.ui.view">
                    <field name="name">sysrental.type_kendaraan.list</field>
                    <field name="model">sysrental.type_kendaraan</field>
                    <field name="arch" type="xml">
                        <list string="type kendaraan" editable="top"> <!-- Tambahkan editable="bottom" atau editable="top" -->
                            <field name="type_kendaraan" optional="show"/>
                        </list>
                    </field>
                </record>
                

        </data>
</odoo>